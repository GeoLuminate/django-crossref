  number = {{ pub.number }}{% endif %}{% if pub.pages %},
  pages = "{{ pub.pages }}"{% endif %}{% if pub.month_bibtex %},
  month = "{{ pub.month_bibtex }}"{% endif %}{% if pub.keywords %},
  keywords = "{{ pub.keywords }}"{% endif %}{% if pub.doi %},
  doi = "{{ pub.doi }}"{% endif %}{% if pub.url %},
